{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "allowJs": true,
        "strict": false,
        "noImplicitAny": false,
        "strictNullChecks": false,// 解决null问题
        "esModuleInterop": true,
        "experimentalDecorators":true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "downlevelIteration": true,
        "lib": ["WebWorker", "DOM", "ES6"],
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "yt-sscs": ["./libs/yt-sscs/yt-sscs"]
        },
        "removeComments": true,
    },
    "include": [
        "src/**/*",
        "@types/**/*",
        "node_modules/vue-tsx-support/enable-check.d.ts",
    ],
    "exclude": [
        "node_modules"
    ]
}
